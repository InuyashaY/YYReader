<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="YToolBar">
        <attr name="android:background"/>
        <attr name="rightIcon" format="dimension"/>
        <attr name="centerTitle" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="CollapsingAvatarToolbar">
        <attr name="collapsedPadding" format="dimension" />
        <attr name="expandedPadding" format="dimension" />

        <attr name="collapsedImageSize" format="dimension" />
        <attr name="expandedImageSize" format="dimension" />

        <attr name="collapsedTextSize" format="dimension" />
        <attr name="expandedTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RefreshLayout">
        <attr name="layout_refresh_empty" format="reference"/>
        <attr name="layout_refresh_error" format="reference"/>
        <attr name="layout_refresh_loading" format="reference"/>
    </declare-styleable>

    <declare-styleable name="EasyRatingBar">
        <attr name="rateNum" format="integer"/>
        <attr name="rating" format="integer"/>
        <attr name="rateNormal" format="reference"/>
        <attr name="rateSelect" format="reference"/>
        <attr name="rateInterval" format="dimension"/>
        <attr name="isIndicator" format="boolean"/>
    </declare-styleable>
</resources>